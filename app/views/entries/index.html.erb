<h1> Entries </h1>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Entry</th>
      <th>Keywords</th>
    </tr>
  </thead>

  <tbody>
    <% @entries.each do |entry| %>
      <tr>
        <td><%= entry.timestamp %></td>
        <td><%= entry.input %></td>
        <td><%= entry.keywords %></td>
        <td><%= link_to 'Show', entry %></td>
        <td><%= link_to 'Destroy', entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
